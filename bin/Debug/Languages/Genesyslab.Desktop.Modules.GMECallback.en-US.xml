  <!--<Value Id="GMECallback.ToastInformation.CallbackAddedSuccessfully" : Information message displayed in Toast when a Callback has been added successfully />-->
  <!--<Value Id="GMECallback.ToastInformation.CallbackAddedSuccessfully" : - 0: phone number to callback />-->
  <!--<Value Id="GMECallback.ToastInformation.CallbackAddedSuccessfully" : - 1: scheduled date />-->
  <!--<Value Id="GMECallback.ToastInformation.CallbackAddedSuccessfully" : - 2: scheduled time />-->
  <Value Id="GMECallback.ToastInformation.CallbackAddedSuccessfully" Text="Callback to {0} scheduled on {1} at {2} (Local Time)"/>
  <!--<Value Id="GMECallback.ToastInformation.ImmediateCallbackAddedSuccessfully" : Information message displayed in Toast when an Immediate Callback has been added successfully />-->
  <!--<Value Id="GMECallback.ToastInformation.ImmediateCallbackAddedSuccessfully" : - 0: phone number to callback />-->
