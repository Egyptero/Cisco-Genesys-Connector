  <Value Id="GMECallback.AddNewCallbackContainerView.ButtonOK" Content="OK" Name="OK"/>
  <!--<Value Id="GMECallback.AddNewCallbackContainerView.ButtonCancel : Label of the Cancel button in new callback dialog-->
  <Value Id="GMECallback.AddNewCallbackContainerView.ButtonCancel" Content="Cancel" Name="Cancel"/>

